	<style>
		.practice-typing-video-container {
			opacity: 0.9;
			/* mix-blend-mode: screen; */
			display: block;
			margin: auto;
			width: 100%;
			margin-top: 100px;
			border-radius: 23px;
			position: relative;
			z-index: 1000;
			background-color: transparent;
		}

		.practice-typing-video {
			width: 100%;
			border-radius: inherit;
			z-index: 1001;
		}

		.practice-typing-video-overlay {
			border-radius: inherit;
			position: absolute;
			width: 100%;
			height: 100%;
			background-color: #1114;
			opacity: 0;
			z-index: 100000;
			transition: 1s ease;
		}

		.practice-typing-video-overlay:hover {
			opacity: 0.9;
		}

		.practice-typing-video-overlay h1 {
			position: absolute;
			left: 50%;
			top: 50%;
			transform: translate(-50%, -50%);

			font-size: 1.5rem;
			opacity: 0.95;
			cursor: pointer;
			transition: font-weight 0.25s ease;

			display: flex;
			flex-direction: row;
			flex-wrap: nowrap;
			justify-content: space-evenly;
			gap: 0.25em;
			align-items: center;
		}

		.practice-typing-video-overlay h1 * {
			display: inline-block;
		}

		.practice-typing-video-overlay h1:hover {
			font-weight: bold;
		}

		.video-label {
			position: absolute;
			left: 50%;
			top: 50%;
			width: 100%;
			height: 100%;

			vertical-align: middle;
			transform: translate(-50%, -50%);

			text-shadow: 2px 2px 4px #1114;
			font-size: 1.5rem;
			opacity: 0.9;
			text-align: center;
			padding-top: 0.5em;
		}

		.practice-typing-video:hover>.video-label {
			opacity: 0;
		}

		.toggle-video-volume {
			display: block;
			position: absolute;
			right: 1.25em;
			top: 1.25em;

			color: #f7f7f7;
			font-size: 1rem;
			cursor: pointer;
			z-index: 1000000;
		}

		.toggle-video-volume:hover {
			color: #eee;
		}

		#particles-js {
			position: absolute;
			/* z-index: 1000000000; */
			z-index: 1;
			height: 100%;
			width: 100%;
			top: 0% !important;
			left: 0% !important;
			border-radius: 0;

			/* transform: translateX(-50%); */

			/* transform: none; */

		}

		.main-banner {
			position: relative;
		}

		.banner-img {
			width: fit-content;
			height: fit-content;
			margin-top: 5em;
			float: right;
			/* border-radius: 50%; */
			border-radius: 23px;
			overflow: hidden;
			opacity: 0.8;
			/* box-shadow: 0px 0px 15px 10px #1115 inset; */
			z-index: 1000;
		}

		.login {
			padding: 0 !important;
			height: 1em !important;
			line-height: 1.2 !important;
			margin-top: 0.5em;
		}
	</style>

